# See https://help.github.com/articles/ignoring-files/ for more about ignoring files.

# Ignore bundler config.
.bundle

# Ignore all logfiles and tempfiles.
/log/*
!/log/.keep
/tmp
!/tmp/.keep

# Ignore pidfiles, but keep the directory.
!/tmp/pids
!/tmp/sockets

# Ignore cache and state files.
/tmp/pids
/tmp/cache
/tmp/sockets
/tmp/storage
/public/system
/public/uploads

# Ignore master key for decrypting credentials and more.
/config/master.key

# Ignore dotenv files
.env
.env.*

# Ignore node_modules
/node_modules
/node_modules/

# Ignore package-lock.json
/package-lock.json

# Ignore other system files
.DS_Store
.tool-versions

# Ignore editor-specific files
*.swp
*~

# Ignore byebug command history
.byebug_history

# Ignore Sprockets cache files
.sprockets-manifest-*.json

# Ignore uploaded files
/storage

# Ignore coverage reports generated by simplecov
/coverage
